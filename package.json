{
  "name": "health-skillz",
  "version": "1.0.0",
  "description": "Claude Skill for analyzing health records via SMART on FHIR",
  "type": "module",
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "bun run --watch src/server.ts",
    "build:brands": "bun run scripts/download-brands.ts",
    "build:skill": "bun run scripts/package-skill.ts",
    "generate-jwks": "bun run scripts/generate-jwks.ts",
    "setup": "bun run build:brands && bun run build:skill && bun run generate-jwks"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3"
  }
}
